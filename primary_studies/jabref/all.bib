@Article{CohenDebrecenyFarewellEtAl2014,
  Title                    = {Issues with the communication and integrity of audit reports when financial reporting shifts to an information-centric paradigm },
  Author                   = {Eric E. Cohen and Roger Debreceny and Stephanie Farewell and Saeed Roohani},
  Journal                  = {International Journal of Accounting Information Systems },
