  Author                   = {Diane Janvrin and Maureen Francis Mascha},
  Journal                  = {International Journal of Accounting Information Systems },
  Year                     = {2014},
  Note                     = {2013 Research Symposium on Information Integrity \&amp; Information Systems Assurance },
