@Article{DebrecenyFarewellPiechockiEtAl2010,
  Title                    = {Does it add up? Early evidence on the data quality of \{XBRL\} filings to the \{SEC\} },
  Author                   = {Roger Debreceny and Stephanie Farewell and Maciej Piechocki and Carsten Felden and André Gräning},
  Journal                  = {Journal of Accounting and Public Policy },
