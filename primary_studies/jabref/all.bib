@Article{LoweLockeLymer2012,
  Title                    = {The SEC's retail investor 2.0: Interactive data and the rise of calculative accountability },
  Author                   = {Alan Lowe and Joanne Locke and Andy Lymer},
  Journal                  = {Critical Perspectives on Accounting },
  Year                     = {2012},
  Note                     = {Accounterability Special Issue for \{CPA\} },
