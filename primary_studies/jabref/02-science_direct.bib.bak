}

@Article{Grant2008,
  Title                    = {Demographic challenges facing the \{CPA\} profession },
  Author                   = {Julia Grant},
  Journal                  = {Research in Accounting Regulation },
  Year                     = {2008},
  Pages                    = {47 - 61},
  Volume                   = {20},
  __markedentry            = {[vagner:6]},
  Abstract                 = {\{ABSTRACT\} Prior research (Young, 1995) using \{AICPA\} and census data illustrated a level of maturity in the accountancy profession, with numbers of \{CPAs\} increasing dramatically within the population from the early 20th century. This work also illustrated strong growth in \{AICPA\} membership, and reported a shift in the activities of the \{AICPA\} members. Corporate practice was growing more quickly and public practice growing only very slowly, sometimes shrinking. The current paper examines 10 additional years of \{AICPA\} data and gathers further economic and demographic U.S. census data to explore these patterns and related issues. The professional membership of the \{AICPA\} continues to have relatively flat growth overall, and public practice has continued to decline relative to other pursuits of \{AICPA\} members. When the data are split into gender and age cohorts, different patterns are detected and challenges identified for continued involvement in the profession as defined by \{AICPA\} membership. },
  DOI                      = {http://dx.doi.org/10.1016/S1052-0457(07)00203-2},
  ISSN                     = {1052-0457},
