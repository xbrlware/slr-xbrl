  ISSN                     = {1467-0895},
  Keywords                 = {Ontology},
  URL                      = {http://www.sciencedirect.com/science/article/pii/S1467089512000589}
}

@Article{Hajnal2015,
  Title                    = {Appraisal of Work-in-Progress Buildings },
  Author                   = {István Hajnal},
